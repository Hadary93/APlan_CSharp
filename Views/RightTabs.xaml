<UserControl x:Class="APLan.Views.RightTabs"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:APLan.Views"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <TabControl>
        <TabItem Header="symbols"  BorderThickness="5" Height="20">
            <local:SymbolsTab></local:SymbolsTab>
        </TabItem>
        <TabItem Header="Selected Items"  BorderThickness="5" Height="20">
            <ScrollViewer HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto">
                <local:SelectedItems></local:SelectedItems>
            </ScrollViewer>
        </TabItem>
    </TabControl>
</UserControl>
